{
    "configurations": [
        {
            "name": "Debug Main Process",
            "type": "node",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/electron",
            "args": [
                "./js/Backend/Main.js",
                "--remote-debugging-port=9222"
            ],
            "outputCapture": "std",
            "sourceMaps": true,
            "protocol": "inspector",
        },
        {
            "name": "Attach - Debug Renderer Process",
            "type": "chrome",
            "request": "attach",
            "port": 9222,
            "sourceMaps": true,
        },
    ],
    "compounds": [
        {
            "name": "Debug app",
            "configurations": [
                "Debug Main Process",
                "Attach - Debug Renderer Process"
            ]
        }
    ]
}